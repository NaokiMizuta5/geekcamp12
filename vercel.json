{
    "builds": [
      {
        "src": "backend/server/wsgi.py",
        "use": "@vercel/python"
      },
      {
        "src": "frontend/package.json",
        "use": "@vercel/next"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "backend/server/wsgi.py"
      }
    ]
  }